#!/bin/bash

result=$(git diff origin/main --name-only | cut -d "/" -f 1,2 | uniq)

echo $result

namespaces=""
for namespace in $result; do
  if [[ "$namespace" == *"namespaces"* ]]; then
    name=$(echo $namespace | cut -d "/" -f 2)
    namespaces+="${name} " 
  fi
done

final=$(echo $namespaces | xargs)

echo "::set-output name=NAMESPACES::$final"
